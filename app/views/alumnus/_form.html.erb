<%= simple_form_for(@alumnu) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :date_of_birth %>
    <%= f.input :place_of_birth %>
    <%= f.input :religion %>
    <%= f.input :gender %>
    <%= f.input :faculty %>
    <%= f.input :sub_faculty %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
